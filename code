#include <stdio.h>

void fibonacci(int n) {
    int a = 0, b = 1, c;
    printf("%d %d ", a, b);
    for (int i = 2; i < n; i++) {
        c = a + b;
        printf("%d ", c);
        a = b;
        b = c;
    }
    printf("\n");
}

int main() {
    int n = 10;
    printf("Fibonacci series:\n");
    fibonacci(n);
    return 0;
}
